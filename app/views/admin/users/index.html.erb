<h3> All Users </h3>
<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td>
        <%= button_to "Delete", admin_user_path(user), method: :delete, data:
        {confirm: "Are you sure you want to delete #{user.username}?"} %>
      </td>
    </tr>
  <% end %>
</table>
